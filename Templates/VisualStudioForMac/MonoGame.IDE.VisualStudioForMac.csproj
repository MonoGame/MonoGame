       <AddinInfo Include="[assembly: AddinAuthor(&quot;@MonoGameTeam&quot;)]" />
     </ItemGroup>
     <WriteLinesToFile File="Properties\AddinInfo.cs" Lines="@(AddinInfo)" Overwrite="true" />
