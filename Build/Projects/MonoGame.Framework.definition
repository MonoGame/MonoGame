      <RemoveDefine>DIRECTX;WINDOWS_MEDIA_SESSION;WINDOWS;MONOMAC;LINUX</RemoveDefine>
      <AddDefine>OPENGL;OPENAL;GLES;ANGLE</AddDefine>
      <Platforms>Windows,MacOS,Linux,Angle</Platforms>
      <Conflicts>OpenGLGraphics,DirectXGraphics,WebGraphics</Conflicts>
      <Requires>OpenALAudio,_GLCompatible</Requires>
    </Service>
    <Service Name="OpenGLGraphics">
      <RemoveDefine>DIRECTX;WINDOWS_MEDIA_SESSION</RemoveDefine>
      <AddDefine>OPENGL;OPENAL</AddDefine>
      <Platforms>iOS,MacOS,Linux,Windows,WindowsGL,Android,tvOS</Platforms>
      <Conflicts>ANGLEGraphics,DirectXGraphics,WebGraphics</Conflicts>
