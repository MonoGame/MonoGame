    <Service Name="DirectXGraphics">
      <Platforms>Windows,WindowsUniversal</Platforms>
      <Conflicts>ANGLEGraphics,OpenGLGraphics,WebGraphics</Conflicts>
      <Requires>XAudioAudio</Requires>
    </Service>
    <Service Name="WebGraphics">
      <Platforms>Web</Platforms>
